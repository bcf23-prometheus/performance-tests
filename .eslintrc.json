{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": "airbnb-base",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"indent": [
			"error",
			2
		],
		"max-len": [
			2,
			{
				"code": 120,
				"ignoreUrls": true,
				"ignoreStrings": true,
				"ignoreTemplateLiterals": true
			}
		],
		"comma-dangle": 0,
		"object-curly-newline": [
			2,
			{
				"consistent": true
			}
		],
		"quotes": [
			2,
			"single",
			{
				"allowTemplateLiterals": true
			}
		],
		"import/prefer-default-export": 0,
		"import/no-default-export": 2,
		"no-console": 0,
		"no-param-reassign": [
			"error",
			{
				"ignorePropertyModificationsFor": [
					"vault"
				]
			}
		],
		"no-shadow": 1
	},
	"settings": {
		"import/core-modules": [
			"k6",
			"k6/http"
		]
	},
	"overrides": [
		{
			"files": [
				"*.test.js"
			],
			"rules": {
				"import/prefer-default-export": 2,
				"import/no-default-export": 0
			}
		}
	]
}